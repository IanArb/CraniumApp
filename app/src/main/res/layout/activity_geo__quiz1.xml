<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:paddingTop="5dp"
    android:paddingBottom="5dp"
    tools:context="com.cranium.ianarbuckle.craniumapp.Geography.Geo_Quiz1">

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/british_flag"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/british_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/irish_flag"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:contentDescription="@string/irish_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/swedish_flag"
        android:layout_below="@+id/british_flag"
        android:layout_alignLeft="@+id/british_flag"
        android:layout_alignStart="@+id/british_flag"
        android:contentDescription="@string/swedish_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/swiss_flag"
        android:layout_below="@+id/irish_flag"
        android:layout_alignLeft="@+id/irish_flag"
        android:layout_alignStart="@+id/irish_flag"
        android:contentDescription="@string/swiss_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/danish_flag"
        android:layout_below="@+id/swedish_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/danish_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/german_flag"
        android:layout_above="@+id/italian_flag"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:contentDescription="@string/german_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/polish_flag"
        android:layout_below="@+id/danish_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/polish_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/italian_flag"
        android:layout_below="@+id/danish_flag"
        android:layout_alignLeft="@+id/german_flag"
        android:layout_alignStart="@+id/german_flag"
        android:contentDescription="@string/italian_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/spanish_flag"
        android:layout_below="@+id/polish_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/spanish_flag" />

    <ImageView
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/french_flag"
        android:layout_below="@+id/polish_flag"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:contentDescription="@string/french_flag" />

    <ImageView
        android:background="@drawable/british_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag1"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:contentDescription="British" />

    <ImageView
        android:background="@drawable/french_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag2"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/irish_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag3"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/spanish_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag4"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/swedish_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag5"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/italian_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag6"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/swiss_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag7"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/polish_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag8"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/danish_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag9"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <ImageView
        android:background="@drawable/german_flag"
        android:layout_width="187dp"
        android:layout_height="75dp"
        android:id="@+id/Flag10"
        android:layout_below="@+id/spanish_flag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:visibility="gone" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Match Flag And Country"
        android:id="@+id/Prompt_Text"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 1"
        android:id="@+id/Country1"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/swedish_flag"
        android:layout_alignRight="@+id/british_flag"
        android:layout_alignEnd="@+id/british_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 2"
        android:id="@+id/Country2"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_above="@+id/swiss_flag"
        android:layout_alignLeft="@+id/irish_flag"
        android:layout_alignStart="@+id/irish_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 3"
        android:id="@+id/Country3"
        android:layout_below="@+id/british_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/danish_flag"
        android:layout_alignRight="@+id/british_flag"
        android:layout_alignEnd="@+id/british_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 4"
        android:id="@+id/Country4"
        android:layout_below="@+id/irish_flag"
        android:layout_alignLeft="@+id/swiss_flag"
        android:layout_alignStart="@+id/swiss_flag"
        android:layout_above="@+id/german_flag"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 5"
        android:id="@+id/Country5"
        android:layout_below="@+id/swedish_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/danish_flag"
        android:layout_alignRight="@+id/swedish_flag"
        android:layout_alignEnd="@+id/swedish_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 6"
        android:id="@+id/Country6"
        android:layout_below="@+id/swedish_flag"
        android:layout_toRightOf="@+id/swedish_flag"
        android:layout_alignBottom="@+id/german_flag"
        android:layout_alignRight="@+id/swiss_flag"
        android:layout_alignEnd="@+id/swiss_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 7"
        android:id="@+id/Country7"
        android:layout_below="@+id/danish_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/spanish_flag"
        android:layout_alignRight="@+id/polish_flag"
        android:layout_alignEnd="@+id/polish_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 8"
        android:id="@+id/Country8"
        android:layout_alignTop="@+id/polish_flag"
        android:layout_toRightOf="@+id/polish_flag"
        android:layout_above="@+id/french_flag"
        android:layout_alignRight="@+id/italian_flag"
        android:layout_alignEnd="@+id/italian_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 9"
        android:id="@+id/Country9"
        android:layout_below="@+id/polish_flag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/spanish_flag"
        android:layout_alignRight="@+id/polish_flag"
        android:layout_alignEnd="@+id/polish_flag" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Country 10"
        android:id="@+id/Country10"
        android:layout_below="@+id/italian_flag"
        android:layout_alignLeft="@+id/italian_flag"
        android:layout_alignStart="@+id/italian_flag"
        android:layout_alignBottom="@+id/french_flag"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
